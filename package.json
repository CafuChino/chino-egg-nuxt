{
  "name": "chino-egg-nuxt",
  "version": "1.0.0",
  "description": "Nuxt project with egg by chino.",
  "private": true,
  "egg": {
    "declarations": false
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "@sentry/vue": "^6.8.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "nuxt": "^2.15.7",
    "sass-loader": "^10.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nuxt/types": "^2.15.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": ">=7.11.0",
    "eslint-config-egg": "^7.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^7.0.4",
    "prettier": "^1.19.1",
    "stylelint": "^13.0.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "cross-env eslint . --fix && nuxt build ./nuxt && BROWSER_BASE_URL=api egg-scripts start --title=egg-server-chino-egg-nuxt",
    "stop": "egg-scripts stop --title=egg-server-chino-egg-nuxt",
    "dev": "cross-env  eslint . --fix && BROWSER_BASE_URL=api  egg-bin dev",
    "debug": "cross-env DEBUG_MODE=FULL egg-bin debug",
    "debug:server": "cross-env DEBUG_MODE=SERVER egg-bin debug",
    "build": "nuxt build ./nuxt",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "CafuChino",
  "license": "MIT"
}
